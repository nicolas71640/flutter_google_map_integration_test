    "version": "0.2.0",
    "configurations": [
        {
            "name": "Frontend: Launch App",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "toolArgs": [
                "-v"
            ]
        },
        {
            "name": "Run",
            "type": "dart",
            "request": "launch",
            "program": "./integration_test/",
            "codeLens": {
                "path": "integration_test/",
                "for": [
                    "run-test",
                    "run-test-file"
                ]
            },
            "toolArgs": [
                "-v"
            ]
        },
        {
            "name": "Debug",
            "type": "dart",
            "request": "launch",
            "program": "./integration_test/",
            "codeLens": {
                "for": [
                    "debug-test",
                    "debug-test-file"
                ]
            },
            "toolArgs": [
                "-v"
            ]
        },
    ]
}